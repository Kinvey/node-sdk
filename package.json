{
  "name": "kinvey-node-sdk",
  "version": "3.8.0",
  "description": "Kinvey JavaScript SDK for Node.js applications.",
  "homepage": "http://www.kinvey.com",
  "bugs": {
    "url": "https://github.com/Kinvey/node-sdk/issues"
  },
  "license": "Apache-2.0",
  "author": "Kinvey, Inc.",
  "main": "./dist/index.js",
  "module": "./src/index.js",
  "contributors": [
    "Thomas Conner <thomas@kinvey.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Kinvey/node-sdk"
  },
  "scripts": {
    "build": "npm run clean && npm run transpile",
    "clean": "rm -rf dist && rm -rf coverage",
    "cover": "istanbul cover _mocha -- --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/index test",
    "docs": "esdoc -c esdoc.json",
    "lint": "npm run lint:src",
    "lint:src": "eslint src/**",
    "lint:test": "eslint test/**",
    "preversion": "rm -rf node_modules && npm install && npm test",
    "postversion": "git push && git push --tags",
    "test": "mocha --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/index test",
    "test:watch": "mocha -w --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/index test",
    "transpile": "babel --no-comments --out-dir dist src"
  },
  "dependencies": {
    "es6-promise": "4.1.0",
    "kinvey-js-sdk": "3.8.0",
    "lodash": "4.17.4",
    "request": "2.81.0"
  },
  "peerDependencies": {
    "kinvey-js-sdk": "3.8.0"
  },
  "devDependencies": {
    "app-module-path": "2.2.0",
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-eslint": "7.1.1",
    "babel-plugin-inline-dotenv": "^1.1.1",
    "babel-plugin-module-resolver": "2.5.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "esdoc": "0.5.2",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "3.17.1",
    "eslint-config-airbnb-base": "11.1.1",
    "eslint-import-resolver-babel-module": "3.0.0",
    "eslint-plugin-import": "2.2.0",
    "expect": "^1.20.2",
    "istanbul": "1.1.0-alpha.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.0",
    "nock": "9.0.9",
    "regenerator-runtime": "0.10.3"
  },
  "engines": {
    "node": "^6.0.0"
  },
  "keywords": [
    "Kinvey",
    "Node.js",
    "NodeJS",
    "Node"
  ]
}
